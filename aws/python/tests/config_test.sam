AWSTemplateFormatVersion: '2010-09-09'
Transform: AWS::Serverless-2016-10-31
Description: SAM file for NATPunch configuration test
Globals:
    Function:
        Timeout: 3
        Runtime: python3.7

Resources:
    Function:
        Type: AWS::Serverless::Function 
        Properties:
            CodeUri: lambda_config_test.py
            Handler: lambda_config_test.lambda_handler
            Layers:
              - "arn:aws:lambda:us-east-1:293602984666:layer:ServerlessNetworking-Python3:5"
              - "arn:aws:lambda:us-east-1:293602984666:layer:PyWebsockets:1"
